);\n\n      await schema.validate(data, {\n        abortEarly: false,\n      });\n      axios.post('http://127.0.0.1:8000/api/users', data).then((response) => {\n        addToast({\n          type: 'success',\n          title: 'Usuário cadastrado!',\n          description: 'Usuário cadastrado com sucesso!',\n        });\n      }).catch((error) => {\n        addToast({\n          type: 'success',\n          title: 'Usuário cadastrado!',\n          description: 'Usuário cadastrado com sucesso!',\n        });\n      });\n\n\n\n      // history.push('/posts');\n      // const userStorage = JSON.parse(localStorage.getItem('@controller:user') || '{}');\n      addToast({\n        type: 'success',\n        title: 'Login realizado com sucesso!',\n        description: 'Seja bem vindo!',\n      });\n    } catch (err) {\n      if (err instanceof Yup.ValidationError) {\n        const errors = getValidationErrors(err);\n        formRef.current?.setErrors(errors);\n        return;\n      }\n      addToast({\n        type: 'error',\n        title: 'Erro na autenticação',\n        description: 'Ocorreu um erro ao fazer login, cheque as credenciais.',\n      });\n    }\n  }\n\n  return (\n    <>\n      <Container>\n        <div id=\"row\">\n          <div>\n            <h1>Preencha o formulario para se cadastrar no sistema</h1>\n            <div id=\"form\">\n              <Form ref={formRef} onSubmit={handleSubmit}>\n                <Input name=\"name\" icon={FaRegUser} placeholder=\"Nome Completo\" />\n                <Input type=\"email\" name=\"email\" icon={RiMailLine} placeholder=\"E-mail\" />\n                <Input name=\"cpf\" icon={RiFileUserLine} placeholder=\"CPF\" />\n                <Input type=\"password\" name=\"password\" icon={RiLockPasswordLine} placeholder=\"Senha\" />\n                <Input type=\"password_confirmation\" name=\"password_confirmation\" icon={RiLockPasswordLine} placeholder=\"Confirmar senha\" />\n                <div id=\"buttonSubmit\">\n                  <Button type=\"submit\" color=\"#2AA9E0\" shadow=\"#2AA9E0\">Confirmar</Button>\n                </div>\n              </Form>\n            </div>\n          </div>\n          <div id=\"cadastrar\">\n            <Link className=\"color-green retirarLink\" to={`/login`}>Login</Link>\n          </div>\n        </div>\n      </Container>\n      <GlobalStyles />\n    </>\n  );\n};\n\nexport default Login;","/home/thayronpc/Documentos/php/front-simeon/src/pages/register/styles.ts",[],"/home/thayronpc/Documentos/php/front-simeon/src/pages/notFound/index.tsx",[],"/home/thayronpc/Documentos/php/front-simeon/src/pages/notFound/styles.ts",[],"/home/thayronpc/Documentos/php/front-simeon/src/pages/post/index.tsx",["138","139","140"],"/home/thayronpc/Documentos/php/front-simeon/src/pages/post/styles.ts",[],"/home/thayronpc/Documentos/php/front-simeon/src/container/navigation/index.tsx",[],"/home/thayronpc/Documentos/php/front-simeon/src/container/navigation/styles.ts",[],{"ruleId":"141","replacedBy":"142"},{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","severity":1,"message":"146","line":47,"column":13,"nodeType":"147","messageId":"148","endLine":47,"endColumn":23},{"ruleId":"149","severity":1,"message":"150","line":53,"column":6,"nodeType":"151","endLine":53,"endColumn":8,"suggestions":"152"},{"ruleId":"145","severity":1,"message":"153","line":69,"column":9,"nodeType":"147","messageId":"148","endLine":69,"endColumn":16},{"ruleId":"145","severity":1,"message":"154","line":27,"column":9,"nodeType":"147","messageId":"148","endLine":27,"endColumn":16},{"ruleId":"145","severity":1,"message":"155","line":7,"column":10,"nodeType":"147","messageId":"148","endLine":7,"endColumn":19},{"ruleId":"145","severity":1,"message":"156","line":7,"column":32,"nodeType":"147","messageId":"148","endLine":7,"endColumn":41},{"ruleId":"145","severity":1,"message":"157","line":8,"column":10,"nodeType":"147","messageId":"148","endLine":8,"endColumn":23},{"ruleId":"145","severity":1,"messa